{
  "name": "personal-blog",
  "version": "1.0.0",
  "description": "Pavel Sušický's blog mostly about web technologies",
  "repository": "git@github.com:Thesoreon/personal-blog.git",
  "author": "Pavel Sušický <pavels.sus@gmail.com>",
  "license": "MIT",
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "scripts": {
    "start": "gatsby develop -o -p 8080",
    "build": "gatsby build",
    "serve": "gatsby serve -o",
    "gatsby:info": "gatsby info -C",
    "gatsby:clean": "gatsby clean",
    "test:type-check": "tsc",
    "test:lint": "eslint --ignore-path .gitignore .",
    "test": "jest -c test/jest.config.js --rootDir",
    "test:watch": "jest --watch -c test/jest.config.js --rootDir",
    "test:coverage": "jest -c test/jest.config.js --rootDir --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "gatsby": "^2.13.31",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.5",
    "@types/jest": "^24.0.15",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-preset-gatsby": "^0.2.7",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
